{
  "name": "Russian transliterator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Russian transliteration for the common man. By Sorin, Aniqah, and Adam",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "css": ["bootstrap.min.css"],
      "js": ["jquery.js", "myscript.js"]
    }
  ]
}